generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:postgres@localhost:5432/socialmedia"
}

model User {
  id Int @id @default(autoincrement())
  username String @unique
  password String
}